<div class="container">
  <p>This <%= name %> list is awesome
    <strong><%= name + ' ' + 'is awesome' %></strong>
  </p>

  <ul>
    <% list.forEach((item) => { %>
      <li><%= item %></li>
    <% }) %>
  </ul>

  <button<% if (![null,undefined].includes(`btn btn-${'light' || 'default'}`)) { %> class="<%= `btn btn-${'light' || 'default'}` %>"<% } %>>Submit</button>
</div>

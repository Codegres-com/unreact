<div>
  <ul>
    <% locals.list.forEach((item) => { %>
      <li><%= item %></li>
    <% }) %>
  </ul>

  <ul>
    <% locals.list.forEach((item, index) => { %>
      <li><%= item %> at <%= index %></li>
    <% }) %>
  </ul>

  <ul>
    <% locals.list.forEach((item, index, array) => { %>
      <li><%= item %> at <%= index %> of <%= array.join(', ') %></li>
    <% }) %>
  </ul>
</div>
